//5，

#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
fun(int x)
{
	long int i, y;
	y = 1;
	for (i = 1; i < x + 1; i++)
	{
		y = y * i;
	}
	return y;
}
int main()
{
	long int i, x, y;
	y = 0;
	scanf("%d", &x);
	for (i = 1; i < x + 1; i++)
	{
		y = y + fun(i);
	}
	printf("%d", y);
	return 0;
}





//6，

#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
	int arr[10]={0};
	int i=0,j=0,t=0;
	scanf("%d %d %d %d %d %d %d %d %d %d", &arr[0], &arr[1], &arr[2], &arr[3], &arr[4], &arr[5], &arr[6], &arr[7], &arr[8], & arr[9]);
    for (i = 0; i <= 8; i++)
        for (j = i + 1; j <= 9; j++)
        {
            int* q = &arr[i];
            int* p = &arr[j];
            if (*q > *p)
            {
                t = *q;
                *q = *p;
                *p = t;
            }
        }
    printf("%d", arr[9]);
		return 0;
}
