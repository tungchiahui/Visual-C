//三角形判断

#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
	int a, b, c, d;
		scanf("%d %d %d",&a,&b,&c);
		if ((a + b > c) && (a + c > b) && (b + c > a))
		{
			if (a == b && b == c && c == a)
				printf("1\n");
        //else if ((a==b&&a!=c&&b!=c) || (a==c&&a!=b&&b!=c) || (b==c&&b!=a&&c!=a))
			else if(a==b||b==c||c==a)                  //因为if已经是检验等边了，所以等边在此式子中已经不检验了。
				printf("2\n");
			else
				printf("3\n");
		}
		else
			printf("0\n");
		return 0;
}	

